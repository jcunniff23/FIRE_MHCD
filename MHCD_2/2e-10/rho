FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2e-10";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 1 0 0 1 0];

internalField   uniform 0;

boundaryField
{
    anode
    {
        type            zeroGradient;
    }
    cathode
    {
        type            zeroGradient;
    }
    bottom
    {
        type            symmetryPlane;
    }
    left
    {
        type            zeroGradient;
    }
    right
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}


